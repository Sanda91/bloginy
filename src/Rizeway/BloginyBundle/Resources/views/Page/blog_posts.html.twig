{% for post in posts|keys %}
    {% include "BloginyBundle:BlogPost:resume.html.twig" with {'post' : posts[post], 'vote' : votes[posts[post].id]|default(false), 'class' : cycle(['odd', 'even'], post) } %}
{% endfor %}

{% if show_pager %}
    {% include "BloginyBundle:Post:pager.html.twig" with {'route' : 'page_blog_posts', 'params' : {'slug': page.slug, 'from': from}, 'target': '#by-blogs'} %}
{% endif %}